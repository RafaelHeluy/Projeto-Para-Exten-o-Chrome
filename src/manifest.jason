{
    "manifest-version": 3,
    "name": "Cronometro",
    "version": 1.0,
    "description": "Marca o tempo quando acionada",
    "icons": {
     "16": "icons/icon16.png",
     "32": "icons/icon32.png",
     "48": "icons/icon48.png",
     "128": "icons/icon128.png"
       },
    "action": {
     "default_popup": "src/popup/popup.html"
    },
    "permissions": ["storage","tabs"],
    "host_permissions": ["https://*/*","http://*/*"]
    
}